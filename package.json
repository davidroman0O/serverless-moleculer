{
  "name": "serverless-moleculer",
  "version": "0.0.1",
  "description": "Serverless Framework handler for Moleculer",
  "main": "index.js",
  "scripts": {
    "pre-test": "npm dedupe && npm prune && npm install",
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "test": "jest --coverage",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "microservice",
    "moleculer",
    "lambda",
    "serverless framework",
    "serverless",
    "framework",
    "aws",
    "aws-lambda"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:davidroman0O/serverless-moleculer.git"
  },
  "homepage": "https://github.com/davidroman0O/serverless-moleculer#readme",
  "author": "David Roman",
  "peerDependencies": {
    "moleculer": "^0.11.0"
  },
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "3.0.0",
    "eslint": "4.8.0",
    "jest": "^20.0.4",
    "jest-cli": "20.0.4",
    "lolex": "^2.1.3",
    "moleculer": "^0.13.3",
    "moleculer-docgen": "0.2.1",
    "nodemon": "1.12.1",
    "npm-check": "5.4.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/services/"
    ]
  },
  "engines": {
    "node": ">= 6.x.x"
  },
  "dependencies": {
    "babel-plugin-transform-define": "^1.3.0",
    "bluebird": "^3.5.3",
    "lodash": "^4.17.11"
  }
}
